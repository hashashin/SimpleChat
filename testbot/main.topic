name main

+"(test $foo=.+)"
    $foo int !max
    0 !ct
    ""
    loop 
        ?ct ?max >= ifleave
        ?ct 1 + !ct
        ?ct + " " +
    endloop trim;
        
    

+"([hello hi] .*)"
    "hi how are you?"
    {
        "(?Iam [good fine well] .*)"
            "Glad to hear it.";
        "(?Iam [bad (not too)] .*)"
            "Oh, I'm sorry";
    } next;

+"$foo=.*"
    "I don't know how to respond to " $foo +;
